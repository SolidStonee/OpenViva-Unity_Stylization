  m_TableData:
  - m_Id: 28667011072
    m_Localized: "        \xD6\u011Freticiye ho\u015Fgeldin.        "
    m_Metadata:
      m_Items: []
  - m_Id: 31921790976
    m_Localized: "\xD6\u011Freticisen \xE7\u0131kmak i\xE7in ye\u015Fil dairenin
      d\u0131\u015F\u0131na \xE7\u0131k."
    m_Metadata:
      m_Items: []
  - m_Id: 35088490496
    m_Localized: "Men\xFCy\xFC a\xE7mak i\xE7in ESC'ye bas"
    m_Metadata:
      m_Items: []
  - m_Id: 38607511552
    m_Localized: "MENU tu\u015Funa basarak men\xFCy\xFC a\xE7"
    m_Metadata:
      m_Items: []
  - m_Id: 2429243351040
    m_Localized: "Kontrollerden VR'a ge\xE7ebilirsin."
    m_Metadata:
      m_Items: []
  - m_Id: 2432250667008
    m_Localized: "VR ellerini kalibre etmeni \xF6neririz."
    m_Metadata:
      m_Items: []
  - m_Id: 2434586894336
    m_Localized: "Devam etmeye haz\u0131rsan Yap\u0131lacaklara bas."
    m_Metadata:
      m_Items: []
  - m_Id: 2436520468480
    m_Localized: "A\u015Fa\u011F\u0131daki g\xF6revleri tamamla."
    m_Metadata:
      m_Items: []
  - m_Id: 2561028382720
    m_Localized: "Devam etmeye haz\u0131rsan b\xFCt\xFCn men\xFCleri kapat."
    m_Metadata:
      m_Items: []
  - m_Id: 2562844516352
    m_Localized: "Y\xFCr\xFCmek i\xE7in WASD kullan. Ko\u015Fmak i\xE7in Sol Shift'e
      bas."
    m_Metadata:
      m_Items: []
  - m_Id: 2563947618304
    m_Localized: "trackpad/thumbstick kullanarak y\xFCr\xFC."
    m_Metadata:
      m_Items: []
  - m_Id: 2564996194304
    m_Localized: "trackpad/thumbstick kullanarak \u0131\u015F\u0131nlan."
    m_Metadata:
      m_Items: []
  - m_Id: 2791434084352
    m_Localized: Bir karakterle birlikte yere oturursan
    m_Metadata:
      m_Items: []
  - m_Id: 2793766117376
    m_Localized: "Ctrl'ye basarak \xE7\xF6mel."
    m_Metadata:
      m_Items: []
  - m_Id: 2795221540864
    m_Localized: "Karakterle etkile\u015Fimde bulunmak i\xE7in el komutlar\u0131
      kullanabilirsin."
    m_Metadata:
      m_Items: []
  - m_Id: 2796727296000
    m_Localized: R'ye basarak Merhaba de
    m_Metadata:
      m_Items: []
  - m_Id: 2902734135296
    m_Localized: "\u0130\u015Faret \xE7\u0131kana kadar elini iki yana salla"
    m_Metadata:
      m_Items: []
  - m_Id: 2905242329088
    m_Localized: "G\xFCzel!"
    m_Metadata:
      m_Items: []
  - m_Id: 2907104600064
    m_Localized: "\u015Eimdi birine gelmesi i\xE7in komut ver."
    m_Metadata:
      m_Items: []
  - m_Id: 2909117865984
    m_Localized: "F'ye basarak seni takip etmesini sa\u011Fla"
    m_Metadata:
      m_Items: []
  - m_Id: 3032682061824
    m_Localized: "Avu\xE7 i\xE7in sana bakarken elini ileri geri sallayarak takip
      etmesini sa\u011Fla."
    m_Metadata:
      m_Items: []
  - m_Id: 3035588714496
    m_Localized: "G\xFCzel! B\xF6ylece karakterin seni takip edecek."
    m_Metadata:
      m_Items: []
  - m_Id: 3037870415872
    m_Localized: "Ortadaki plastik orde\u011Fi al."
    m_Metadata:
      m_Items: []
  - m_Id: 3042324766720
    m_Localized: "Sol t\u0131k sol eldir. Sa\u011F t\u0131k ise sa\u011F el."
    m_Metadata:
      m_Items: []
  - m_Id: 3461121826816
    m_Localized: "E\u015Fyay\u0131 b\u0131rakmak in SHIFT bas\u0131l\u0131 tutarken
      t\u0131kla."
    m_Metadata:
      m_Items: []
  - m_Id: 3463638409216
    m_Localized: "Drop/Pickup :Grip Tu\u015Fu"
    m_Metadata:
      m_Items: []
  - m_Id: 3628822683648
    m_Localized: "\u015Eimdi \xF6rde\u011Fi vermek i\xE7in ileri uzat"
    m_Metadata:
      m_Items: []
  - m_Id: 3668991533056
    m_Localized: "Sol elini uzatmak i\xE7in Q, sa\u011F elini uzatmak i\xE7in E'ye
      bas."
    m_Metadata:
      m_Items: []
  - m_Id: 3670279184384
    m_Localized: "Bu \u015Fekilde onlara bir \u015Feyler verebilir, ya da isteyebilirsin."
    m_Metadata:
      m_Items: []
  - m_Id: 3672086929408
    m_Localized: "Yerdeki foto\u011Fraf\u0131 al"
    m_Metadata:
      m_Items: []
  - m_Id: 3775463940096
    m_Localized: "ve kalan e\u015Fyalar\u0131 b\u0131rak"
    m_Metadata:
      m_Items: []
  - m_Id: 3781289828352
    m_Localized: "Foto\u011Fraf\u0131 y\u0131rtmak i\xE7in tuttu\u011Fun elinin tu\u015Funa
      bas"
    m_Metadata:
      m_Items: []
  - m_Id: 3783965794304
    m_Localized: "Foto\u011Fraf\u0131 y\u0131rtmak i\xE7in iki elini yak\u0131nla\u015Ft\u0131r"
    m_Metadata:
      m_Items: []
  - m_Id: 3861891768320
    m_Localized: "iki elinle tuttu\u011Fun zaman,"
    m_Metadata:
      m_Items: []
  - m_Id: 3862835486720
    m_Localized: "iki fare tu\u015Funa da ayn\u0131 anda bas."
    m_Metadata:
      m_Items: []
  - m_Id: 3863795982336
    m_Localized: "iki TRIGGER tu\u015Funa bas ve ellerini uzakla\u015Ft\u0131r"
    m_Metadata:
      m_Items: []
  - m_Id: 3935703130112
    m_Localized: "\xD6\u011Fretici Tamamland\u0131!"
    m_Metadata:
      m_Items: []
  - m_Id: 3937355685888
    m_Localized: "Oyun geli\u015Ftirme a\u015Famas\u0131nda, buglar olabilir!"
    m_Metadata:
      m_Items: []
  - m_Id: 3938685280256
    m_Localized: "\u015Eimdi eski yerine g\xF6nderileceksin."
    m_Metadata:
      m_Items: []
  - m_Id: 22083718340608
    m_Localized: "Bas\u0131l\u0131 tutarak ko\u015F."
    m_Metadata:
      m_Items: []
  - m_Id: 30314129776640
    m_Localized: onunla oyunlar oynayabilirsin.
    m_Metadata:
      m_Items: []
